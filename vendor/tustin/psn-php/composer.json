{
    "name": "tustin/psn-php",
    "license": "MIT",
    "description": "PHP wrapper for the PlayStation API.",
    "homepage": "https://github.com/Tustin/psn-php",
    "keywords": ["playstation", "api", "php"],
    "autoload": {
        "psr-4": {
            "Tustin\\PlayStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "php": "^8.1",
        "tustin/haste": "^1.1",
        "nesbot/carbon": "^2.41"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "phpunit": "composer dump-autoload && vendor/bin/phpunit"
    }
}
